<%- if controller_name != 'sessions' %>
  <%= link_to "Ingresar", new_session_path(resource_name), class: "forgot" %>
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Registrarse", new_registration_path(resource_name), class: "forgot" %>
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "¿Olvido su contraseña?", new_password_path(resource_name), class: "forgot", style: "padding-top: 0px ! important;"%>
<% end -%>
